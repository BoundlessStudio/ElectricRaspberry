    <PackageReference Include="Microsoft.SemanticKernel.Connectors.Memory.AzureCognitiveSearch" Version="0.15.0" />
    <PackageReference Include="Microsoft.SemanticKernel.Connectors.Memory.Postgres" Version="0.15.0" />
    <PackageReference Include="Microsoft.SemanticKernel.Connectors.Memory.Qdrant" Version="0.15.0" />
    <PackageReference Include="Microsoft.SemanticKernel.Connectors.AI.OpenAI" Version="0.15.0" />
    <PackageReference Include="Microsoft.SemanticKernel.Planning.ActionPlanner" Version="0.15.0" />
